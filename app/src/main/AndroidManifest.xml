<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcc.serviceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <!--Define a MainActivity como a principal-->
                <action android:name="android.intent.action.MAIN" />
                <!--Define a MainActivity como a primeira a executar-->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.LoginActiviy"/>
        <activity android:name=".activity.CadastroActivity"/>
        <activity android:name=".activity.EnderecoActivity"/>
    </application>

</manifest>